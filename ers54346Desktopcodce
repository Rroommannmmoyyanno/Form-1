[32m+[m[32m    try { return self._compile(schemaObj); }[m
[32m+[m[32m    catch(e) {[m
[32m+[m[32m      if (e instanceof MissingRefError) return loadMissingSchema(e);[m
[32m+[m[32m      throw e;[m
