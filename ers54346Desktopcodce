[32m+[m[32m            if (!added(ref)) self.addSchema(sch, ref, undefined, meta);[m
[32m+[m[32m          });[m
[32m+[m[32m        }[m
[32m+[m[32m      }).then(function() {[m
[32m+[m[32m        return _compileAsync(schemaObj);[m
[32m+[m[32m      });[m
[32m+[m
